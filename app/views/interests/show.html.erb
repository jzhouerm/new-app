<h2> All Events that match your interest: </h2>
<% @interest.events.each do |event| %>
    <h2> <%= link_to event.name, event_path(event) %> </h2>
    <h4> location: <%=event.location.city %> </h4>
     <h4> Date: <%= event.date %></h4>
     <img src="<%= event.img_url %>" alt="<% event.name %>" style= "width:100px; height:100px; border:10;">
     <h4> Description: <%= event.description %></h4>
     <br>
     <br>
     <% end %>
     <br>
     <br>
     
     