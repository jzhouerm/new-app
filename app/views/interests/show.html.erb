<h2> All Events that match your interest: </h2>
<% @interest.events.each do |event| %>

    <h3> <%= link_to event.name, event_path(event) %> </h3> <br>
    <h4> Location: <%=event.location.city %> </h4>
     <h4> Date: <%= event.date.strftime("%A, %B %d, %Y") %></h4>
     <img src="<%= event.img_url %>" alt="<% event.name %>" style= "width:220px; height:160px; border:10;">

     <h4> Description: <%= event.description %></h4>
     <br>
     <br>
     <% end %>
     <br>
     <br>
     
     